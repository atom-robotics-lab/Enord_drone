<node name="controller_spawner" pkg="controller_manager" type="spawner" respawn="false" output="screen" ns="pushpak" args="Revolute 14_position_controller Revolute 15_position_controller Revolute 16_position_controller Revolute 17_position_controller joint_state_controller "/>
